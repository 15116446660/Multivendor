<!DOCTYPE html>
<!--[if IE]><![endif]-->
<!--[if IE 8 ]><html dir="{{ direction }}" lang="{{ lang }}" class="ie8"><![endif]-->
<!--[if IE 9 ]><html dir="{{ direction }}" lang="{{ lang }}" class="ie9"><![endif]-->
<!--[if (gt IE 9)|!(IE)]><!-->
<html dir="{{ direction }}" lang="{{ lang }}">
<!--<![endif]-->
<head>
<meta charset="UTF-8" />
<meta name="viewport" content="width=device-width, initial-scale=1">
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<title>{{ title }}</title>
<base href="{{ base }}" />
{% if description %}
<meta name="description" content="{{ description }}" />
{% endif %}
{% if keywords %}
<meta name="keywords" content="{{ keywords }}" />
{% endif %}
<script src="catalog/view/javascript/jquery/jquery-2.1.1.min.js" type="text/javascript"></script>
<script type="text/javascript" src="catalog/view/theme/multivendor_v1/assets/js/jquery-min.js"></script> 
<link href="catalog/view/javascript/bootstrap/css/bootstrap.min.css" rel="stylesheet" media="screen" />
<script src="catalog/view/javascript/bootstrap/js/bootstrap.min.js" type="text/javascript"></script>
<link href="catalog/view/javascript/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css" />
<link href="catalog/view/theme/multivendor_v1/stylesheet/stylesheet.css" rel="stylesheet">
<link rel="stylesheet" href="catalog/view/theme/multivendor_v1/assets/css/bootstrap.min.css" type="text/css">    
<!-- Font Awesome CSS -->
<link rel="stylesheet" href="catalog/view/theme/multivendor_v1/assets/fonts/font-awesome.min.css" type="text/css">   
<!-- Line Icons CSS -->
<link rel="stylesheet" href="catalog/view/theme/multivendor_v1/assets/fonts/line-icons/line-icons.css" type="text/css">      
<!-- Main Styles -->
<link rel="stylesheet" href="catalog/view/theme/multivendor_v1/assets/css/main.css" type="text/css">

<!-- Animate CSS -->
<link rel="stylesheet" href="catalog/view/theme/multivendor_v1/assets/extras/animate.css" type="text/css">    
<!-- Owl Carousel -->
<link rel="stylesheet" href="catalog/view/theme/multivendor_v1/assets/extras/owl.carousel.css" type="text/css">
<link rel="stylesheet" href="catalog/view/theme/multivendor_v1/assets/extras/owl.theme.css" type="text/css">  
<!-- Slicknav Css -->
<link rel="stylesheet" href="catalog/view/theme/multivendor_v1/assets/css/slicknav.css" type="text/css">  
<!-- Responsive CSS Styles -->
<link rel="stylesheet" href="catalog/view/theme/multivendor_v1/assets/css/responsive.css" type="text/css">
<script src="catalog/view/theme/multivendor_v1/assets/js/multiseller.js" type="text/javascript"></script>
<script src="catalog/view/javascript/swiper.jquery.min.js" type="text/javascript"></script>
{% for style in styles %}
<link href="{{ style.href }}" type="text/css" rel="{{ style.rel }}" media="{{ style.media }}" />
{% endfor %}
{% for script in scripts %}
<script src="{{ script }}" type="text/javascript"></script>
{% endfor %}
<script src="catalog/view/javascript/common.js" type="text/javascript"></script>
{% for link in links %}
<link href="{{ link.href }}" rel="{{ link.rel }}" />
{% endfor %}
{% for analytic in analytics %}
{{ analytic }}
{% endfor %}
</head>
<body>

<div class="header">
      <!-- Start Top Bar -->
      <div class="top-bar">
        <div class="container">
          <div class="row">
            <div class="col-md-4 col-sm-2">
              <div class="language-wrapper">
              <div class="box-language">
              {{ language }} 
              </div>          
              <div class="box-currency">
                {{ currency }}
              </div>
              <div class="box-telephone">
               <div class="pull-right">
              <i class='fa fa-phone'></i> {{ telephone }}</div></div>
              </div>
                            
              <div class="clear"></div>              
            </div>

            <div class="col-md-8 col-sm-10">
              <!-- shopping cart end -->
              <div class="search-area">{{ search }}</div>
              <div class="shop-cart">
                <ul>
                    <li>
                      <a class="cart-btn" href="{{ wishlist }}" id="wishlist-total" title="{{ text_wishlist }}">
                        <i class="fa fa-heart"></i>  
                      </a>
                    </li>
                    <li>
                      <a class="cart-btn" href="{{ seller_href }}" title="{{ text_selleraccount }}" target="_blank"><i class="fa fa-user-plus"></i>
                         <span>{{ text_selleraccount }}</span>
                      </a>
                    </li>
                     <li>
                      <a class="cart-btn" href="{{ shopping_cart }}" title="{{ text_shopping_cart }}"><i class="icon-basket"></i>
                        <span>{{ text_shopping_cart }}</span>
                      </a>

                      
                      
                      </li>
                    <li>
                       <a class="cart-btn" href="{{ account }}" title="{{ text_account }}"  data-toggle="dropdown"><i class="fa fa-user" ></i>
                       <span>{{ text_account }}</span> 
                       <span class="caret"></span></a>
                        <ul class="dropdown-menu animated fadeIn">
                          {% if logged %}
                          <li><a href="{{ account }}">{{ text_account }}</a></li>
                          <li><a href="{{ order }}">{{ text_order }}</a></li>
                          <li><a href="{{ transaction }}">{{ text_transaction }}</a></li>
                          <li><a href="{{ download }}">{{ text_download }}</a></li>
                          <li><a href="{{ logout }}">{{ text_logout }}</a></li>
                          {% else %}
                          <li><a href="{{ register }}">{{ text_register }}</a></li>
                          <li><a href="{{ login }}">{{ text_login }}</a></li>
                          {% endif %}
                        </ul>
                      </li>
                </ul>
              </div>
            </div>
          </div>
          <!-- end row -->
        </div>
          <!-- end container -->
      </div>
        <!-- end top-bar-->
      {{ menu }}
</div>

